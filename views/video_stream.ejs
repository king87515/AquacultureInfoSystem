<script>
	function LoadVideo() {
		$.ajax({
			type: "GET",
			dataType: 'json',
			url: "/stream",
			async: true,
			success: function (data) {
				console.log("<%= host %>");
				console.log("<%= url %>");
				$("#stream").attr("src", "<%= url %>");
			},
			error: function (xmlhttprequest, textstatus, message) {
				console.log("<%= host %>");
				console.log("<%= url %>");
				console.log(xmlhttprequest);
			}
		});
	}

